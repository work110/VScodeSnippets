            "    <frame src=\"t/1.html\" name=\"bottom\" ></frame>\r",
            "</frameset>\r",
            "\r",
            "<!-- 在框架内页面设置指定跳转链接 <a href=\"t/2.html\"  target=\"main\">跳转</a> -->"
